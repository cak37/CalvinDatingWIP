<?xml version="1.0" encoding="utf-8"?>
<ScrollView
xmlns:android="http://schemas.android.com/apk/res/android"
android:id="@+id/ScrollView01"
android:layout_width="match_parent"
android:layout_height="wrap_content"
android:scrollbars="vertical">
<LinearLayout
    android:layout_width="fill_parent"
    android:orientation="vertical"
    android:layout_height="fill_parent">

    <TextView
        android:id="@+id/TextViewTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Survey"
        android:textSize="10pt">
    </TextView>

    <EditText
        android:id="@+id/EditTextName"
        android:layout_height="wrap_content"
        android:hint="What is your full name?"
        android:inputType="textPersonName"
        android:layout_width="fill_parent">
    </EditText>

    <View android:layout_width="match_parent" android:layout_height="1dp" android:background="@android:color/darker_gray"/>


    <EditText
        android:id="@+id/EditTextUserName"
        android:layout_height="wrap_content"
        android:hint="What is your prefered username?"
        android:inputType="textPersonName"
        android:layout_width="fill_parent">
    </EditText>

    <View android:layout_width="match_parent" android:layout_height="1dp" android:background="@android:color/darker_gray"/>

    <EditText
        android:id="@+id/EditTextBirthday"
        android:layout_height="wrap_content"
        android:hint="What is your Birthday?"
        android:inputType="date"
        android:layout_width="fill_parent">
    </EditText>

    <View android:layout_width="match_parent" android:layout_height="1dp" android:background="@android:color/darker_gray"/>

    <EditText
        android:id="@+id/EditTextMajor"
        android:layout_height="wrap_content"
        android:hint="What is your major?"
        android:inputType="textPersonName"
        android:layout_width="fill_parent">
    </EditText>

    <View android:layout_width="match_parent" android:layout_height="1dp" android:background="@android:color/darker_gray"/>

    <TextView
        android:text="Current class standing?"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView12"
        android:textSize="18sp" />
    <Spinner
        android:id="@+id/SpinnerFeedbackTypeClass"
        android:layout_height="wrap_content"
        android:hint="Current class standing?"
        android:layout_width="fill_parent"
        android:entries="@array/feedbacktypeClass">
    </Spinner>

    <View android:layout_width="match_parent" android:layout_height="1dp" android:background="@android:color/darker_gray"/>

    <EditText
        android:id="@+id/EditTextMBTI"
        android:layout_height="wrap_content"
        android:hint="What is your Meyers Brigg's result?"
        android:inputType="text"
        android:layout_width="fill_parent">
    </EditText>

    <View android:layout_width="match_parent" android:layout_height="1dp" android:background="@android:color/darker_gray"/>

    <TextView
        android:text="What are the 5 points of Calvinism?"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/EditText12"
        android:layout_height="wrap_content"
        android:hint="Point of Calvinism #1"
        android:inputType="text"
        android:layout_width="fill_parent">
    </EditText>
    <EditText
        android:id="@+id/EditText13"
        android:layout_height="wrap_content"
        android:hint="Point of Calvinism #2"
        android:inputType="text"
        android:layout_width="fill_parent">
    </EditText>
    <EditText
        android:id="@+id/EditText14"
        android:layout_height="wrap_content"
        android:hint="Point of Calvinism #3"
        android:inputType="text"
        android:layout_width="fill_parent">
    </EditText>
    <EditText
        android:id="@+id/EditText15"
        android:layout_height="wrap_content"
        android:hint="Point of Calvinism #4"
        android:inputType="text"
        android:layout_width="fill_parent">
    </EditText>
    <EditText
        android:id="@+id/EditText16"
        android:layout_height="wrap_content"
        android:hint="Point of Calvinism #5"
        android:inputType="text"
        android:layout_width="fill_parent">
    </EditText>

    <View android:layout_width="match_parent" android:layout_height="1dp" android:background="@android:color/darker_gray"/>

    <EditText
        android:id="@+id/EditText17"
        android:layout_height="wrap_content"
        android:hint="Favorite place on Calvin's campus?"
        android:inputType="text"
        android:layout_width="fill_parent">
    </EditText>

    <View android:layout_width="match_parent" android:layout_height="1dp" android:background="@android:color/darker_gray"/>

    <TextView
        android:text="How much do you passionately dislike Hope College?"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        android:textSize="18sp" />
    <Spinner
        android:id="@+id/SpinnerFeedbackTypeHate"
        android:layout_height="wrap_content"
        android:hint="How much do you passionately dislike Hope College?"
        android:layout_width="fill_parent"
        android:entries="@array/feedbacktypeHate">
    </Spinner>

    <View android:layout_width="match_parent" android:layout_height="1dp" android:background="@android:color/darker_gray"/>

    <TextView
        android:text="Which dining hall is your favorite?"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:textSize="18sp" />
    <Spinner
        android:id="@+id/SpinnerFeedbackTypeDining"
        android:layout_height="wrap_content"
        android:hint="Which dining hall is your favorite?"
        android:layout_width="fill_parent"
        android:entries="@array/feedbacktypeDining">
    </Spinner>

    <View android:layout_width="match_parent" android:layout_height="1dp" android:background="@android:color/darker_gray"/>

    <TextView
        android:text="Favorite Chapel day?"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:textSize="18sp" />
    <Spinner
        android:id="@+id/SpinnerFeedbackTypeChapel"
        android:layout_height="wrap_content"
        android:hint="Favorite Chapel day?"
        android:layout_width="fill_parent"
        android:entries="@array/feedbacktypeChapel">
    </Spinner>

    <View android:layout_width="match_parent" android:layout_height="1dp" android:background="@android:color/darker_gray"/>

    <TextView
        android:text="LOFT?"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:textSize="18sp" />
    <RadioButton
        android:id="@+id/CheckBoxResponse1"
        android:layout_height="wrap_content"
        android:text="Always"
        android:layout_width="fill_parent">
    </RadioButton>
    <RadioButton
        android:id="@+id/CheckBoxResponse2"
        android:layout_height="wrap_content"
        android:text="Sometimes"
        android:layout_width="fill_parent">
    </RadioButton>
    <RadioButton
        android:id="@+id/CheckBoxResponse3"
        android:layout_height="wrap_content"
        android:text="Never"
        android:layout_width="fill_parent">
    </RadioButton>

    <View android:layout_width="match_parent" android:layout_height="1dp" android:background="@android:color/darker_gray"/>

    <Button
        android:id="@+id/ButtonSendFeedback"
        android:layout_height="wrap_content"
        android:text="Submit"
        android:layout_width="fill_parent">
    </Button>

</LinearLayout>
</ScrollView>